{
  "name": "shacop",
  "author": "Ahmet ilhan <ahmetilhan.dev@gmail.com>",
  "publisher": "Ahmetilhan",
  "displayName": "Shacop",
  "description": "Code partial sharing with teams discord whatsapp ",
  "repository": "https://github.com/ahmetilhan24/vscode-shacop.git",
  "version": "1.1.2",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "assets/icon.png",
  "keywords": [
    "Share",
    "Code sharing",
    "Whatsapp",
    "Teams"
  ],
  "activationEvents": [
    "onCommand:shacop.helloWorld"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "shacop.helloWorld",
        "title": "Share Selected Code"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceLangId == javascript",
          "command": "shacop.helloWorld"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "opn": "^6.0.0"
  }
}